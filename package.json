{
  "name": "jamquery-node",
  "version": "0.1.0",
  "engines": {
    "node": "10.14.1"
  },
  "description": "Jamquery Node.js backend",
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "scripts": {
    "start": "source config/setup-env.sh; nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "serve": "source config/setup-env.sh; export NODE_ENV=production; node dist/index.js"
  },
  "author": "tura",
  "license": "MIT",
  "dependencies": {
    "ejs": "^2.6.1",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "jshint": "^2.9.6",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@types/express": "^4.16.0"
  }
}
