{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "no-unused-vars": [
      "error",
      {
        "args": "all",
        "argsIgnorePattern": "^_"
      }
    ],
    "no-use-before-define": "off"
  }
}